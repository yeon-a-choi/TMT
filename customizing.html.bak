 <script>
   /* 디자이너1 팝업 */
    var img = document.getElementsByClassName('designer1');
   for (var x=0; x<img.length; x++ ){
      img.item(x).onclick=function() {
         window.open("custo_designer-1.html","designer-1", "width=800px, height=550px, left=350px, top=70px")
      };/* 팝업창주소, 이름, 크기 위치 */
   }


   /* best 슬라이드 */
   var slides = document.querySelector('.slide'),
      slide = document.querySelectorAll('.slide li'),
      currentIdx = 0,
      slideCount = slide.length,
      prevBtn = document.querySelector('.custo_prev'),
      nextBtn = document.querySelector('.custo_next'),
      slideWidth = 150,
      slideMarginR = 30,
      slideMarginL = 30;
      slides.style.width = (slideWidth + slideMarginR + slideMarginL) * slideCount + 'px';
   function moveSlide(num) {
      slides.style.left = -num * 210 + 'px';
      currentIdx = num;
   }
   nextBtn.addEventListener('click', function() {
      if(currentIdx < slideCount - 4) {
         moveSlide(currentIdx + 1);
      }
      else {
         moveSlide(0);
      }
   } );
   prevBtn.addEventListener('click', function() {
      if(currentIdx > 0) {
         moveSlide(currentIdx - 1);
      }
      else {
         moveSlide(slideCount - 4);
      }
   } );

   
   /* 뷰티 더보기 */
   $(function() {
      $('.b_more').on('click', function() {
         if ($(this).children().is('.b_btn')) {
            $(this).html('<p class="close">닫기</p>').addClass('b_hide');
            $(this).parent().removeClass('slide-up').addClass('slide-down');
         }
         else {
            $(this).html('<p class="b_btn">더보기</p>').removeClass('b_hide');
            $(this).parent().removeClass('slide-down').addClass('slide-up');
            window.scrollTo(0, 400);
         }
      });
   });


   /* 패션 더보기 */
   $(function() {
      $('.f_more').on('click', function() {
         if ($(this).children().is('.f_btn')) {
            $(this).html('<p class="close">닫기</p>').addClass('f_hide');
            $(this).parent().removeClass('slide-up').addClass('slide-down');
         }
         else {
            $(this).html('<p class="f_btn">더보기</p>').removeClass('f_hide');
            $(this).parent().removeClass('slide-down').addClass('slide-up');
            window.scrollTo(0, 800);
         }
      });
   });


   /* 디자이너 더보기 */
   $(function() {
      $('.d_more').on('click', function() {
         if ($(this).children().is('.d_btn')) {
            $(this).html('<p class="close">닫기</p>').addClass('d_hide');
            $(this).parent().removeClass('slide-up').addClass('slide-down_d');
            $().css()
         }
         else {
            $(this).html('<p class="d_btn">더보기</p>').removeClass('d_hide');
            $(this).parent().removeClass('slide-down_d').addClass('slide-up');
            window.scrollTo(0, 1450);
         }
      });
   });



   /* 뷰티 페이지내 위치이동 */
   function scroll_b() {
      window.scrollTo(0, 400);
   }
   /* 패션 페이지내 위치이동 */
   function scroll_f() {
      window.scrollTo(0, 800);
   }
   /* 디자이너 페이지내 위치이동 */
   function scroll_d() {
      window.scrollTo(0, 1450);
   }



 </script>


